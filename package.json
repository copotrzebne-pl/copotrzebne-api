{
  "name": "copotrzebne-api",
  "version": "1.3.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:docker:db": "docker-compose -p copotrzeba_db --env-file .env -f docker-compose.db.yml up",
    "start:docker:test-db": "docker-compose -f docker-compose.db-test.yml up -d",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"migrations/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"migrations/**/*.{ts,js}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.{ts,js}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node --experimental-vm-modules node_modules/.bin/jest --coverage --config ./jest-e2e.js --runInBand",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:import": "PGPASSWORD=\"$(cat .env| grep API_DB_PWD | cut -d'=' -f 2)\" pg_restore -c -U $(cat .env| grep API_DB_USERNAME | cut -d'=' -f 2) -p $(cat .env| grep API_DB_PORT | cut -d'=' -f 2) -h localhost --no-owner --no-privileges -d $(cat .env| grep API_DB_DATABASE | cut -d'=' -f 2) ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.6",
    "@types/umzug": "2.3.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.3.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sequelize": "^6.28.2",
    "sequelize-typescript": "^2.1.3",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^8.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/luxon": "^2.3.0",
    "@types/node": "^14.14.36",
    "@types/pg": "^8.6.5",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "umzug": "^2.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
